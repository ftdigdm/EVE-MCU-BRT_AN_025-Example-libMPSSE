{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: Build EVE Example",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/nologo",
                "/DUSE_MPSSE=0",
                "/Fe:",
                "\"${workspaceFolder}\\.vscode\\build\\FT8xx-SPI.exe\"",
                "/Fo.\\.vscode\\",
                "/I",
                "\"${workspaceFolder}\"",
                "/I",
                "\"${workspaceFolder}\\example\"",
                "/I",
                "\"${workspaceFolder}\\Library\\eve\\include\"",
                "/I",
                "\"${workspaceFolder}\\release\\include\"",
                "/I",
                "\"${workspaceFolder}\\release\\libftd2xx\"",
                "main.c",
                ".\\Library\\eve\\source\\EVE_API.c",
                ".\\Library\\eve\\source\\EVE_HAL.c",
                ".\\Library\\eve\\ports\\eve_libmpsse\\EVE_libmpsse.c",
                ".\\example\\*.c",
                ".\\release\\build\\libmpsse.lib"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ]
}